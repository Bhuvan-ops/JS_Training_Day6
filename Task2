function performOperation(num1, num2, callback) {
    return callback(num1, num2);
}
function add(a, b) {
    return a + b;
}
function multiply(a, b) {
    return a * b;
}

const sum = performOperation(5, 3, add);
const product = performOperation(5, 3, multiply);

console.log("Sum:", sum);
console.log("Product:", product);
